        "1663779252838b7eb3-1": {
          "type": "text_block",
          "settings": {
            "image": "shopify:\/\/shop_images\/MG_1663.jpg",
            "image_link": "",
            "bg_position": "center-center",
            "title": "WAY BEYOND CLEAN",
            "text": "<p>100% plant based formula <br\/><\/p><p>no glycolsa<br\/><\/p><p>no petrochemicals<br\/><\/p><p>no synthetic fragrance<br\/><\/p><p>no added dye or color<br\/><\/p><p>vegan* and cruelty-free<br\/><\/p><p>made by us <br\/><\/p><p>*all of our products are vegan with<br\/>the exception of those containing honey.<\/p>",
